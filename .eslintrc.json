{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true,
		"jest/globals": true
	},
	"extends": ["airbnb", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["prettier", "jest"],
	"rules": {
		"prettier/prettier": ["error"]
	}
}
